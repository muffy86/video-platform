{
  "name": "AI Remodeling Assistant",
  "short_name": "AI Remodel",
  "description": "Professional AI-powered home remodeling assistant with computer vision and voice control",
  "version": "1.0.0",
  "icons": [
    {
      "src": "/icons/icon-72x72.png",
      "sizes": "72x72",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/icons/icon-96x96.png",
      "sizes": "96x96",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/icons/icon-128x128.png",
      "sizes": "128x128",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/icons/icon-144x144.png",
      "sizes": "144x144",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/icons/icon-152x152.png",
      "sizes": "152x152",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/icons/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/icons/icon-384x384.png",
      "sizes": "384x384",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable any"
    }
  ],
  "start_url": "/",
  "scope": "/",
  "display": "standalone",
  "orientation": "any",
  "theme_color": "#1a1a1a",
  "background_color": "#000000",
  "prefer_related_applications": false,
  "categories": ["productivity", "utilities", "photo"],
  "lang": "en",
  "dir": "ltr",
  "display_override": ["window-controls-overlay", "standalone", "minimal-ui"],
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "permissions": [
    "camera",
    "microphone",
    "storage-access"
  ],
  "protocol_handlers": [
    {
      "protocol": "web+airemodelingassistant",
      "url": "/?action=%s"
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "images",
          "accept": ["image/*"]
        }
      ]
    }
  },
  "shortcuts": [
    {
      "name": "Take Photo",
      "short_name": "Photo",
      "description": "Capture a photo for room analysis",
      "url": "/?action=photo",
      "icons": [
        {
          "src": "/icons/camera-icon.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Voice Assistant",
      "short_name": "Voice",
      "description": "Start voice interaction",
      "url": "/?action=voice",
      "icons": [
        {
          "src": "/icons/mic-icon.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "New Project",
      "short_name": "Project",
      "description": "Start a new remodeling project",
      "url": "/?action=project",
      "icons": [
        {
          "src": "/icons/project-icon.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    }
  ],
  "screenshots": [
    {
      "src": "/screenshots/mobile-main.png",
      "sizes": "390x844",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Main interface on mobile"
    },
    {
      "src": "/screenshots/fold-unfolded.png",
      "sizes": "1080x841",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Interface on unfolded Pixel 9 Pro Fold"
    }
  ],
  "related_applications": [],
  "file_handlers": [
    {
      "action": "/open-image",
      "accept": {
        "image/*": [".jpg", ".jpeg", ".png", ".gif", ".webp"]
      }
    }
  ]
}